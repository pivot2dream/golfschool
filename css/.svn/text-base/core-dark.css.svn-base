.todo-stats {
  *zoom: 1;
  padding-left: 25px;
  margin: 13px auto 0;
  color: #777777;
  width: 18%;

}
  .todo-stats:after {
    content: "\0020";
    display: block;
    height: 0;
    clear: both;
    overflow: hidden;
    visibility: hidden;
  }
  .todo-stats .todo-count {
    float: left;
  }
    .todo-stats .todo-count .number {
      font-weight: bold;
      color: #333333;
    }
  .todo-stats .todo-clear {
    float: right;
  }
    .todo-stats .todo-clear a {
      color: #777777;
      font-size: 12px;
    }
      .todo-stats .todo-clear a:visited {
        color: #777777;
      }
      .todo-stats .todo-clear a:hover {
        color: #336699;
      }
